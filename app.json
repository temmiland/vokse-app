{
  "expo": {
    "name": "Vokse",
    "slug": "vokse",
    "version": "1.0.0",
    "owner": "temmiland",
    "orientation": "portrait",
    "icon": "./assets/images/app-icons/skin_tone_3.png",
    "scheme": "vokse",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "land.temmi.vokse",
      "entitlements": {
        "aps-environment": "development"
      },
      "buildNumber": "5"
    },
    "androidNavigationBar": {
      "barStyle": "light-content"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icons/skin_tone_3_dark.png",
        "backgroundColor": "#ffffff",
        "monochromeImage": "./assets/images/app-icons/skin_tone_3_tinted.png"
      },
      "package": "land.temmi.vokse",
      "versionCode": 5
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@temmiland/expo-dynamic-app-icon",
        {
          "skin_tone_1": {
            "image": "./assets/images/app-icons/skin_tone_1_dark.png",
            "androidImageMonochrome": "./assets/images/app-icons/skin_tone_1_tinted.png",
            "iosImageDark": "./assets/images/app-icons/skin_tone_1_dark.png",
            "iosImageTinted": "./assets/images/app-icons/skin_tone_1_tinted.png",
            "prerendered": true,
            "platforms": [
              "ios",
              "android"
            ],
            "isMainIcon": false
          },
          "skin_tone_2": {
            "image": "./assets/images/app-icons/skin_tone_2_dark.png",
            "androidImageMonochrome": "./assets/images/app-icons/skin_tone_1_tinted.png",
            "iosImageDark": "./assets/images/app-icons/skin_tone_2_dark.png",
            "iosImageTinted": "./assets/images/app-icons/skin_tone_2_tinted.png",
            "prerendered": true,
            "platforms": [
              "ios",
              "android"
            ],
            "isMainIcon": false
          },
          "skin_tone_3": {
            "image": "./assets/images/app-icons/skin_tone_3_dark.png",
            "androidImageMonochrome": "./assets/images/app-icons/skin_tone_1_tinted.png",
            "iosImageDark": "./assets/images/app-icons/skin_tone_3_dark.png",
            "iosImageTinted": "./assets/images/app-icons/skin_tone_3_tinted.png",
            "prerendered": true,
            "platforms": [
              "ios",
              "android"
            ],
            "isMainIcon": true
          },
          "skin_tone_4": {
            "image": "./assets/images/app-icons/skin_tone_4_dark.png",
            "androidImageMonochrome": "./assets/images/app-icons/skin_tone_1_tinted.png",
            "iosImageDark": "./assets/images/app-icons/skin_tone_4_dark.png",
            "iosImageTinted": "./assets/images/app-icons/skin_tone_4_tinted.png",
            "prerendered": true,
            "platforms": [
              "ios",
              "android"
            ],
            "isMainIcon": false
          },
          "skin_tone_5": {
            "image": "./assets/images/app-icons/skin_tone_5_dark.png",
            "androidImageMonochrome": "./assets/images/app-icons/skin_tone_1_tinted.png",
            "iosImageDark": "./assets/images/app-icons/skin_tone_5_dark.png",
            "iosImageTinted": "./assets/images/app-icons/skin_tone_5_tinted.png",
            "prerendered": true,
            "platforms": [
              "ios",
              "android"
            ],
            "isMainIcon": false
          }
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "minSdkVersion": 23,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "kotlinVersion": "1.9.23"
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2e475ad8-aa23-4cd4-bc31-3be9658790d9"
      }
    }
  }
}
